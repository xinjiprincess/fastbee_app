.player-wrapper {
  display: flex;
  place-content: center;
  font-size: 12px;
  width: 100%;
  height: 260px;
}
.player-wrapper .player-display {
  position: absolute;
  left: 4px;
  bottom: 48px;
  display: flex;
  width: 150px;
  height: 30px;
  justify-content: center;
  color: #fff;
}
.player-wrapper .container-shell {
  position: relative;
  width: 100%;
}
.player-wrapper #container {
  background: rgba(13, 14, 27, 0.7);
  width: 100%;
  height: 100%;
  position: relative;
}
.tab_wrap {
  margin-top: 10px;
}
.opt_button_wrap {
  margin-top: 40px;
}
.opt_button_wrap .play_btn_wrap {
  display: inline-block;
  text-align: center;
  flex: 1;
}
.opt_button_wrap .play_btn_wrap .btn_icon-size {
  display: inline-block;
  padding: 8px;
  background-color: #01a394;
}
.opt_button_wrap .play_btn_wrap .btn_icon-size::after {
  border: transparent;
}
.opt_button_wrap .play_btn_wrap .btn_icon {
  display: inline-block;
  padding: 16px;
  border-radius: 50px;
}
.opt_button_wrap .play_btn_wrap .btn_icon::after {
  border: transparent;
}
.opt_button_wrap .play_btn_wrap .btn_title {
  display: block;
  font-size: 14px;
  margin-top: 8px;
}
.opt_button_wrap .live_str_wrap {
  display: flex;
  flex-direction: row;
}
.opt_button_wrap .live_str_item {
  display: flex;
  flex-direction: row;
  margin-top: 60px;
}
.opt_button_wrap .playback_wrap {
  padding: 0 20px;
}
.opt_button_wrap .playback_wrap .button_wrap {
  display: flex;
  flex-direction: row;
  margin-top: 56px;
}

